        "condition": {"test": "datum['count_*'] > 100", "value": "black"},
        "value": "white"
      },
	   "color": {
      "field": "Arrest",
      "type": "nominal"
    },
      "config": {
        "axis": {"shortTimeLabels": true}
      },
